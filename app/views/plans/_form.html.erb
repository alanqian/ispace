<%= simple_form_for(@plan) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :plan_set_id %>
    <%= f.input :category_id %>
    <%= f.input :user_id %>
    <%= f.input :store_id %>
    <%= f.input :num_stores %>
    <%= f.input :fixture_id %>
    <%= f.input :init_facing %>
    <%= f.input :nominal_size %>
    <%= f.input :base_footage %>
    <%= f.input :usage_percent %>
    <%= f.input :published_at %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
