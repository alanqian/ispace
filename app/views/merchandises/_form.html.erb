<%= simple_form_for(@merchandise) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :product_id %>
    <%= f.input :store_id %>
    <%= f.input :user_id %>
    <%= f.input :price %>
    <%= f.input :facing %>
    <%= f.input :run %>
    <%= f.input :volume %>
    <%= f.input :volume_rank %>
    <%= f.input :value %>
    <%= f.input :value_rank %>
    <%= f.input :margin %>
    <%= f.input :margin_rank %>
    <%= f.input :psi %>
    <%= f.input :psi_rank %>
    <%= f.input :psi_by %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
