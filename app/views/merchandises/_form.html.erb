<%= simple_form_for(@merchandise) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :product_id %>
    <%= f.input :store_id %>
    <%= f.input :user_id %>
    <%= f.input :supplier_id %>
    <%= f.input :price %>
    <%= f.input :new_product %>
    <%= f.input :on_promotion %>
    <%= f.input :force_on_shelf %>
    <%= f.input :force_off_shelf %>
    <%= f.input :max_facing %>
    <%= f.input :min_facing %>
    <%= f.input :rcmd_facing %>
    <%= f.input :volume %>
    <%= f.input :vulume_rank %>
    <%= f.input :value %>
    <%= f.input :value_rank %>
    <%= f.input :profit %>
    <%= f.input :profit_rank %>
    <%= f.input :psi %>
    <%= f.input :psi_rank %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
