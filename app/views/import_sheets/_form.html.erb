<%= simple_form_for(@import_sheet) do |f| %>
  <%= f.error_notification %>

  <div class="inputs">
    <%= f.input :store_id %>
    <%= f.input :user_id %>
    <%= f.input :step %>
    <%= f.input :filename %>
    <%= f.input :sheets %>
    <%= f.input :comment %>
    <%= f.input :ext %>
    <%= f.input :sel_sheets %>
    <%= f.input :category_id %>
    <%= f.input :mapping %>
  </div>

  <div class="actions">
    <%= f.button :submit %>
  </div>
<% end %>
