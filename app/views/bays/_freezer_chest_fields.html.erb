<h3>
  <%= f.input_field :name, size: 10, class: 'elem_name wrap bottom_only'  %>
  <%= f.hidden_field :_destroy %>
  <%= f.hidden_field :id %>
  <%= f.hidden_field :level, class: "elem_sort_by" %>
  <%= link_to raw("&times"), "#", title:t("dict.removeFreezerChest"), id: "remove-element",
    class: 'remove_element cmd_ui' %>
</h3>
<div>
  <div class="row hlabel">
    <%= f.input :height, wrapper_html: { class: 'span2' }  %>
  </div>
  <div class="row hlabel">
    <%= f.input :depth , wrapper_html: { class: 'span2' }%>
    <%= f.input :inside_height, wrapper_html: { class: 'span2' }%>
  </div>
  <div class="row hlabel">
    <%= f.input :wall_thick, wrapper_html: { class: 'span2' } %>
    <%= f.input :merch_height, wrapper_html: { class: 'span2' }%>
  </div>
  <div class="row hlabel">
    <%= f.input :color, as: :color, wrapper_html: { class: 'span2'}  %>
  </div>
  <%= f.hidden_field :notch_num %>
  <%= f.hidden_field :from_base %>
</div>

